  "engines": {
    "node": ">=10.x"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "start:hot": "TS_NODE_PROJECT=tsconfig.json env-cmd webpack-dev-server -w --hot --open --content-base dist/",
    "start": "TS_NODE_PROJECT=tsconfig.json env-cmd webpack-dev-server -w --open --content-base dist/",
    "build": "TS_NODE_PROJECT=tsconfig.json NODE_ENV=production env-cmd webpack -p",
    "test": "TS_NODE_PROJECT=tsconfig.json env-cmd mocha --timeout 600000 --require ./babel-hook.js \"./src/**/*.spec.ts\"",
    "lint": "tslint -c tslinton 'src/**/*.ts'",
    "type": "tsc"
  },
  "keywords": [
    "typescript",
    "react",
    "d3"
  ],
